{
  "remaps": {
    "CtrlR": "Win",
    "AltR": "Alt",
    "ShiftR": "Shift",
    "NextToShiftL": "Shift"
  },
  "keyPresses": [
    { "repeat": 1, "inputKey": "Caps", "fire": "Esc" },
    {
      "repeat": 2,
      "inputKey": "Shift",
      "fire": "Caps",
      "skipKeyBindings": true
    }
  ],
  "keybindings": [
    // Alt tab Mode For Windows Like MacOS
    {
      "hotkeys": ["Alt Tab"],
      "when": { "isAltTabMode": false },
      "set": { "isAltTabMode": true },
      "fire": ["Ctrl:up Alt:down Tab", null]
    },
    {
      "hotkeys": ["Alt Tab"],
      "when": { "isAltTabMode": true },
      "fire": ["Tab", null]
    },
    {
      "hotkeys": ["Alt"],
      "when": { "isAltTabMode": false },
      "fire": ["Ctrl:down", "Ctrl:up"]
    },
    {
      "hotkeys": ["Alt"],
      "when": { "isAltTabMode": true },
      "set": { "isAltTabMode": false },
      "fire": [null, "Alt:up"]
    },
    {
      "hotkeys": ["Esc"],
      "when": { "isAltTabMode": true, "Alt": true },
      "set": { "isAltTabMode": false },
      "fire": ["Esc Alt:up Ctrl:down", null]
    },

    // Vim Mode With Caps
    {
      "hotkeys": ["H", "L"],
      "when": { "Caps": true, "Alt": true, "isAltTabMode": false },
      "fire": ["Ctrl:up vimHomeEndKey Ctrl:down", null]
    },
    {
      "hotkeys": ["H", "J", "K", "L"],
      "when": { "Caps": true, "Alt": true, "isAltTabMode": true },
      "fire": ["vimArrowKey", null]
    },
    {
      "hotkeys": ["J", "K"],
      "when": { "Caps": true, "Alt": true },
      "fire": ["vimHomeEndKey", null]
    },
    {
      "hotkeys": ["H", "L"],
      "when": { "Caps": true, "Win": true },
      "fire": ["Ctrl:down vimArrowKey Ctrl:up", null]
    },
    {
      "hotkeys": ["H", "J", "K", "L"],
      "when": { "Caps": true, "Ctrl": true },
      "fire": ["Alt:up vimArrowKey", null]
    },
    {
      "hotkeys": ["H", "J", "K", "L"],
      "when": { "Caps": true },
      "fire": ["vimArrowKey", null]
    },
    {
      "hotkeys": ["S"],
      "when": { "Caps": true },
      "fire": ["Shift:down", "Shift:up"]
    },
    {
      "hotkeys": ["Caps"],
      "when": { "S": true },
      "fire": [null, "Shift:up"]
    },
    {
      // Because non n-rollover keyboard: Win + Caps + S + H/J = F7/Y/]
      "hotkeys": ["F7", "Y", "BracketR"],
      "when": { "Caps": true },
      "fire": [null, null]
    },

    // Right/Left clicks
    {
      "hotkeys": ["Win Caps C"],
      "when": { "C": true, "isClickDown": false },
      "fire": ["RightClick:down", null],
      "set": { "isClickDown": true }
    },
    {
      "hotkeys": ["Win Caps C"],
      "when": { "C": true, "isClickDown": true },
      "fire": [null, null]
    },
    {
      "hotkeys": ["Win Caps C"],
      "when": { "C": false, "isClickDown": true },
      "fire": [null, "RightClick:up"],
      "set": { "isClickDown": false }
    },
    {
      "hotkeys": ["Win C"],
      "when": { "C": true, "isClickDown": false },
      "fire": ["LeftClick:down", null],
      "set": { "isClickDown": true }
    },
    {
      "hotkeys": ["Win C"],
      "when": { "isClickDown": true, "C": true },
      "fire": [null, null]
    },
    {
      "hotkeys": ["Win C"],
      "when": { "isClickDown": true },
      "fire": [null, "LeftClick:up"],
      "set": { "isClickDown": false }
    },
    {
      "hotkeys": ["Win"],
      "when": { "Win": false, "isClickDown": true },
      "fire": [null, "LeftClick:up"],
      "set": { "isClickDown": false }
    },

    // Google Chrome

    {
      "hotkeys": ["Win Alt F"],
      "when": { "appName": "chrome.exe" },
      "fire": ["Ctrl:up F11 Ctrl:down", null]
    },
    {
      "hotkeys": ["Alt Enter"],
      "when": { "appName": "chrome.exe" },
      "fire": ["Ctrl:up Alt:down Enter Alt:up Ctrl:down", null]
    },
    {
      "hotkeys": ["Alt H"],
      "when": { "appName": "chrome.exe" },
      "fire": ["Shift:down Tab Shift:up", null]
    },
    {
      "hotkeys": ["Alt L"],
      "when": { "appName": "chrome.exe" },
      "fire": ["Tab", null]
    },
    {
      "hotkeys": ["Alt Colon"],
      "when": { "appName": "chrome.exe" },
      "fire": ["L", null]
    },


    // General

    { "hotkeys": ["Ctrl"], "fire": ["Alt:down", "Alt:up"] },
    { "hotkeys": ["Caps"], "fire": [null, null] },
    { "hotkeys": ["Win"], "fire": [null, null] },

    {
      "hotkeys": ["Caps Alt V"],
      "fire": ["Ctrl:up Win:down V Win:up Ctrl:down", null]
    },
    {
      "hotkeys": ["Caps C"],
      "fire": ["Ctrl:down C Ctrl:up", null]
    },
    {
      "hotkeys": ["Caps Tab"],
      "fire": ["Ctrl:down Tab Ctrl:up", null]
    },
    {
      "hotkeys": ["Caps Space"],
      "fire": ["Ctrl:down Space Ctrl:up", null]
    },

    {
      "hotkeys": ["Ctrl Tab"],
      "fire": ["Alt:up Ctrl:down Tab Ctrl:up Alt:down", null]
    },

    //     - WinAlt H => Down(keyUpLAlt Win↓ Left↕ Win↑) Up(null)
    // - WinAlt J => Down(keyUpLAlt Win↓ Down↕ Win↑) Up(null)
    // - WinAlt K => Down(keyUpLAlt Win↓ Up↕ Win↑) Up(null)
    // - WinAlt L => Down(keyUpLAlt Win↓ Right↕ Win↑) Up(null)
    //{ "hotkeys": ["Win Alt H"], "fire": ["Alt:down F Alt:up", null] },

    { "hotkeys": ["Win A"], "fire": ["Alt:down NP0 NP2 NP2 NP5 Alt:up", null] },
//     - Win A => Down(Alt↓ NP0↕ NP2↕ NP2↕ NP5↕ Alt↑) Up(null)
// - Win E => Down(Alt↓ NP0↕ NP2↕ NP3↕ NP3↕ Alt↑) Up(null)
// - Win I => Down(Alt↓ NP0↕ NP2↕ NP3↕ NP7↕ Alt↑) Up(null)
// - Win O => Down(Alt↓ NP0↕ NP2↕ NP4↕ NP3↕ Alt↑) Up(null)
// - Win U => Down(Alt↓ NP0↕ NP2↕ NP5↕ NP0↕ Alt↑) Up(null)
// - Win N => Down(Alt↓ NP1↕ NP6↕ NP4↕ Alt↑) Up(null)

    { "hotkeys": ["Win F"], "fire": ["Alt:down F Alt:up", null] },

    { "hotkeys": ["Win D"], "fire": ["Win:down D Win:up", null] },
    { "hotkeys": ["Win Back"], "fire": ["Ctrl:down Back Ctrl:up", null] },
    { "hotkeys": ["Win Space"], "fire": ["Win", null] },

    { "hotkeys": ["Win H"], "fire": ["Alt:down Left Alt:up", null] },
    { "hotkeys": ["Win L"], "fire": ["Alt:down Right Alt:up", null] },

    {
      "hotkeys": ["Alt Back"],
      "fire": ["Ctrl:up Shift:down Home Shift:up Back Ctrl:down", null]
    },
    {
      "hotkeys": ["Alt Esc"],
      "when": { "isAltTabMode": false },
      "fire": [null, null]
    },
    { "hotkeys": ["Alt J"], "fire": ["Ctrl:up Next Ctrl:down", null] },
    { "hotkeys": ["Alt K"], "fire": ["Ctrl:up Prior Ctrl:down", null] },

    { "hotkeys": ["Alt F1"], "fire": ["Ctrl:up F1 Ctrl:down", null] },
    { "hotkeys": ["Alt F2"], "fire": ["Ctrl:up F2 Ctrl:down", null] },
    { "hotkeys": ["Alt F10"], "fire": ["Ctrl:up F10 Ctrl:down", null] },
    { "hotkeys": ["Alt F11"], "fire": ["Ctrl:up F11 Ctrl:down", null] },
    { "hotkeys": ["Alt F12"], "fire": ["Ctrl:up F12 Ctrl:down", null] },

    { "hotkeys": ["F1"], "fire": ["BrightnessDown", null] },
    { "hotkeys": ["F2"], "fire": ["BrightnessUp", null] },
    { "hotkeys": ["F10"], "fire": ["Mute", null] },

    {
      "hotkeys": ["F11"],
      "fire": ["VolumeDown VolumeDown VolumeDown VolumeDown", null]
    },
    {
      "hotkeys": ["F12"],
      "fire": ["VolumeUp VolumeUp VolumeUp VolumeUp", null]
    }
  ],
  "tests": [
    //Alt Tab Mode
    ["Alt:down Tab Alt:up", "Ctrl:down Ctrl:up Alt:down Tab Alt:up"],
    ["Alt:down Tab Tab Alt:up", "Ctrl:down Ctrl:up Alt:down Tab Tab Alt:up"],
    [
      "Alt:down Shift:down Tab Shift:up Alt:up",
      "Ctrl:down Shift:down Ctrl:up Alt:down Tab Shift:up Alt:up"
    ],
    [
      "Alt:down Shift:down Tab Tab Shift:up Alt:up",
      "Ctrl:down Shift:down Ctrl:up Alt:down Tab Tab Shift:up Alt:up"
    ],
    [
      "Alt:down Tab Esc Alt:up",
      "Ctrl:down Ctrl:up Alt:down Tab Esc Alt:up Ctrl:down Ctrl:up"
    ],
    // Key
    ["A", "A"],
    // Key - Down
    ["H:down H:down H:down", "H:down H:down H:down"],
    // Key - Down then Up
    ["H:down H:down H:up", "H:down H:down H:up"],
    // Key - 2 Keys In Order
    ["H:down J H:up", "H:down J:down J:up H:up"],
    // Key - 2 Keys In No Order
    ["H:down J:down H:up J:up", "H:down J:down H:up J:up"],
    // Key - 3 Keys In Order
    ["H:down J:down K J:up H:up", "H:down J:down K:down K:up J:up H:up"],
    // Key - 3 Keys No Order
    ["H:down J:down K H:up J:up", "H:down J:down K:down K:up H:up J:up"],

    ["Alt", "Ctrl"],

    // VIM MODE
    ["Caps", "Esc"],
    ["Caps:down A Caps:up", "A"],
    ["Caps:down H Caps:up", "Left"],
    ["Caps:down L Caps:up", "Right"],
    ["Caps:down J Caps:up", "Down"],
    ["Caps:down K Caps:up", "Up"],
    ["Caps:down Alt:down H Alt:up Caps:up", "Ctrl Home Ctrl"],
    ["Caps:down Alt:down L Alt:up Caps:up", "Ctrl End Ctrl"],
    ["Caps:down S:down H S:up Caps:up", "Shift:down Left Shift:up"],

    // VIM MODE: When Caps is not the first pressed key
    ["T:down Caps:down T:up Caps:up", "T"],
    ["Alt:down Caps:down H Caps:up Alt:up", "Ctrl Home Ctrl"],
    ["Alt:down Caps:down L Caps:up Alt:up", "Ctrl End Ctrl"],

    // Clicks
    ["Win:down C Win:up", "LeftClick"],
    ["Win:down C:down C:down C Win:up", "LeftClick"],
    ["Win:down C:down Win:up C:up", "LeftClick C:up"],
    ["Win:down Caps:down C Caps:up Win:up", "RightClick"],
    ["Win:down Caps:down C:down C:down C Caps:up Win:up", "RightClick"]
  ]
}
